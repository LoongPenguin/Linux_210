void test_rsa(void)
{
	u8_t digest[20];
	u32_t i;

	sha1(sample_data, 4, digest);

	printk("sha1sum: ");
	for(i=0; i<20; i++)
	{
		printk("0x%02x,", digest[i]);
	}
	printk("\r\n");

	printk("rsa verify: ");
	if( rsa_verify(&public_key, &signature[0], &digest[0]) )
		printk("pass\r\n");
	else
		printk("fail\r\n");
}

